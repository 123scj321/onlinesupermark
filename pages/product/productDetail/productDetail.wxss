/*页面修饰  */
.detailContainer{
  width:100%;
  display:block;
}
.detailContext{
  width:100%;
  margin:0 auto;  
}
.titleImage{
  width:750rpx;
  height:750rpx;
}
.titleImage image{
  width:100%;
  height:100%;
}
/*活动  */
.priceContainer{
  width:100%;
  height:130rpx;
  background:rgba(0,0,0,0.3);
  display: flex;
  flex-flow: row;
  justify-content: space-between;
}
.priceLeft{
  width:400rpx;
  /* height:130rpx; */
  overflow: hidden;
  background:red;
  padding-left:40rpx;
  padding-top:10rpx;
}
.newPrice{
  font-size:32rpx;
  color:#fff;
  font-weight: bold;
}
.newPrice text{
  font-size:48rpx;
}
.oldActivity{
  width:320rpx;
  height:40rpx;
  display: flex;
  flex-flow: row;
  line-height: 40rpx;
  vertical-align: center;
  justify-content: space-between;
}
.oldPrice{
  text-decoration:line-through;
  padding-left:20rpx;
  color:#fff;
  font-size:28rpx;
  font-weight: bold;
}
.secondContainer{
  width:180rpx;
  height:40rpx;
  display: flex;
  flex-flow: row;
  /* justify-content: space-between; */
  border:4rpx solid #fff;
   padding-left: 20rpx; 
  text-align:center;
}
.watch{
  font-size:28rpx;
  color:#fff;
}
.secondContainer text{

  font-size:32rpx;
  color:#fff;
}
.priceCenter{
    width: 0;
    height: 0;
    border-top: 126rpx solid red;
    border-right: 50rpx solid transparent;
}
.priceRight{
  width:260rpx;
  height:100%;
}
.priceRight{
  margin:25rpx auto; 
  color:#fff;
  width:200rpx;
  height:40rpx;
  font-size:32rpx;
  font-weight: bold;
}
.start{
  text-align: center;
  width:100%;
}
.Time{
  font-size:36rpx;
}
.Time text{
  padding:2rpx 4rpx;
  background: #666;
  border-radius: 5rpx;
}
/*详情信息  */
.detailContainer{
  background-color:#fff;
  width:100%;
  height:220rpx;
}
.detail{
  width:94%;
  margin:0 auto;
  font-size:36rpx;
}
.price{
  width:94%;
  margin:0 auto;
  color:orangered;
  font-size:32rpx;
  font-weight: bold;
}
.price text{
  font-size:40rpx;
}
.other{
  margin-left:20rpx;
  display: flex;
  flex-flow: row;
  width:400rpx;
  font-size:36rpx;
}
/*图片制作  */
.detailImages{
  width:100%;
  height: 80rpx;
  display: flex;
  flex-flow: row;
  justify-content: center;
  color:#ccc;
  font-size:28rpx;
  line-height: 80rpx;
}
.detailImages text{
  margin:0 10rpx;
}
.lineLeft,.lineRight{
  border-bottom:4rpx solid #ccc;
  width:150rpx;
  height:40rpx;
  margin-bottom: -40rpx;
  margin-left:20rpx;
  margin-right:20rpx;
}
/*图片列表页  */
.ImageList{
  background: #fff;
  width:100%;
  margin:0 auto;
  padding: 0 auto;
  text-align: center;
}
.ImageText{
  height:80rpx;
  font-size:28rpx;
  width:94%;
  line-height: 80rpx;
  margin:0 auto;
  border-bottom: 1px solid #ccc;
  text-align: left;
}
.ImageList image{
  width:94%;
  margin:10px auto;
  height:600rpx;
}
/*底部样式  */
.footer{
  display: flex;
  flex-flow: row;
  justify-content: center;
  width:100%;
  position: fixed;
  bottom: 0rpx;
  background: #f0f0f0;
  height:100rpx;
  border-top:4rpx solid #666;
}
.cart,.store{
  padding: 10rpx;
  text-align: center;
  border-right: 1px solid #666;
  width:120rpx;
  font-size:28rpx;
  color:#666;
}
.cart view,.store view{
  font-size:40rpx;
  color:#666;
}
.addCart,.nowBuy{
  width:250rpx;
  background: orangered;
  color:#fff;
  text-align: center;
  line-height: 100rpx;
  border-left:1px solid #fff;
}
.addCart{
background: orange;
}
